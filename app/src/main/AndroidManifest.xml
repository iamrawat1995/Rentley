<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example1.ankitrawat.rentley">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example1.ankitrawat.rentley.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.example1.ankitrawat.rentley.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example1.ankitrawat.rentley.Login" />
        <activity android:name="com.example1.ankitrawat.rentley.Register" />
        <activity android:name="com.example1.ankitrawat.rentley.Home" />
        <activity
            android:name="com.example1.ankitrawat.rentley.Main_home"
            android:label="@string/title_activity_main_home"></activity>

    </application>

    </manifest>